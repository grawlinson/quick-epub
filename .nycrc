{
  "statements": 50,
  "lines": 50,
  "branches": 50,
  "functions": 50,
  "watermarks": {
    "statements": [ 50, 80 ],
    "lines": [ 50, 80 ],
    "functions": [ 50, 80 ],
    "branches": [ 50, 80 ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "coverage",
    "dist",
    "test",
    "**/*.test.js",
    "**/node_modules/**"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "require": [
    "@babel/register"
  ],
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "all": true,
  "check-coverage": true,
  "report-dir": "./coverage"
}
